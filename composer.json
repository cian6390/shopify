{
    "name": "cian/shopify",
    "minimum-stability": "stable",
    "description": "Simple Shopify API package for PHP、Laravel。",
    "type": "library",
    "license": "MIT",
    "keywords": ["shopify", "php", "laravel"],
    "authors": [
        {
            "name": "Cian.h",
            "email": "cian.h@evolutivelabs.com"
        }, {
            "name": "Erin.l",
            "email": "erin.l@evolutivelabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cian\\Shopify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cian\\Shopify\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "^5|^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1",
        "symfony/var-dumper": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cian\\Shopify\\ShopifyServiceProvider"
            ],
            "aliases": {
                "Shopify": "Cian\\Shopify\\ShopifyFacade",
                "ShopifyFacade": "Cian\\Shopify\\ShopifyMacroFacade"
            }
        }
    }
}
